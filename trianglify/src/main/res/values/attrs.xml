<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TrianglifyView">
        <attr name="bleedX" format="dimension" />
        <attr name="bleedY" format="dimension" />
        <attr name="cellSize" format="dimension" />
        <attr name="variance" format="dimension" />
        <attr name="gridType" format="enum">
            <enum name="rectangle" value="0" />
            <enum name="circle" value="1" />
            <enum name="triangle" value="2" />
        </attr>
        <attr name="fillTriangle" format="boolean" />
        <attr name="fillStrokes" format="boolean" />
        <attr name="strokeSize" format="float" />
        <attr name="randomColoring" format="boolean" />
        <attr name="fillViewCompletely" format="boolean" />
        <attr name="palette" format="enum">
            <enum name="Yl" value="0" />
            <enum name="YlGn" value="1" />
            <enum name="YlGnBu" value="2" />
            <enum name="GnBu" value="3" />
            <enum name="BuGn" value="4" />
            <enum name="PuBuGn" value="5" />
            <enum name="PuBu" value="6" />
            <enum name="BuPu" value="7" />
            <enum name="RdPu" value="8" />
            <enum name="PuRd" value="9" />
            <enum name="OrRd" value="10" />
            <enum name="YlOrRd" value="11" />
            <enum name="YlOrBr" value="12" />
            <enum name="Purples" value="13" />
            <enum name="Blues" value="14" />
            <enum name="Greens" value="15" />
            <enum name="Oranges" value="16" />
            <enum name="Reds" value="17" />
            <enum name="Greys" value="18" />
            <enum name="PuOr" value="19" />
            <enum name="BrBG" value="20" />
            <enum name="PRGn" value="21" />
            <enum name="PiYG" value="22" />
            <enum name="RdBu" value="23" />
            <enum name="RdGy" value="24" />
            <enum name="RdYlBu" value="25" />
            <enum name="Spectral" value="26" />
            <enum name="RdYlGn" value="27" />
        </attr>
    </declare-styleable>
</resources>
